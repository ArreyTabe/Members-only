<h3>All Posts</h3>


<table class="table table-hover table-bordered">
    <tr class="table-active">
        <th>Title</th><br>
        <th>Description</th>
    </tr>
    <% @posts.each do |post| %>
    <tr>
        <td><%= post.title %></td>
        <td><%= post.description %></td>
    </tr>
    <% end %>
</table>


